{
    "containerDefinitions": [
        {
            "name": "topic-classification-runner",
            "image": "368882958480.dkr.ecr.us-east-1.amazonaws.com/dev-topic-classification:latest",
            "cpu": 2048,
            "memory": 4096,
            "essential": true,
            "environment": [
                {
                    "name": "CI",
                    "value": "true"
                },
                {
                    "name": "PER_COLLECTION_LIMIT",
                    "value": 10
                },
                {
                    "name": "ACTIVE_THOUGHT_COLLECTIONS",
                    "value": "yt_test_topic_classification,news_test_topic_classification,people_test_topic_classification"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "MONGO_CONNECTION_STRING",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:368882958480:secret:dev/ml/topic-classification-Ppr6yt:MONGO_CONNECTION_STRING::"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:368882958480:secret:dev/ml/topic-classification-Ppr6yt:OPENAI_API_KEY::"
                },
                {
                    "name": "ANTHROPIC_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:368882958480:secret:dev/ml/topic-classification-Ppr6yt:ANTHROPIC_API_KEY::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "ecs/dev/ml/topic-classification",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "dev-ml-topic-classification",
    "executionRoleArn": "arn:aws:iam::368882958480:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
  }